{
  "extends": [
    "next/core-web-vitals",
    "next",
    "prettier",
    "plugin:@tanstack/eslint-plugin-query/recommended",
    "eslint:recommended",
    "google"
  ],
  "plugins": [
    "import",
    "@tanstack/query"
  ],
  "rules": {
    "no-console": "warn",
    "no-unused-vars": "warn",
    "@next/next/no-document-import-in-page": "off",
    "no-undef": "off",
    "object-curly-spacing": "off",
    "require-jsdoc": "off",
    "max-len": "off",
    "indent": "off",
    "no-useless-catch": "off",
    "camelcase": "off",
    "new-cap": "off",
    "operator-linebreak": "off",
    "quotes": "off",
    "quote-props": "off",
    "react/display-name": "off"
  }
}